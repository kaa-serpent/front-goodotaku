{"ast":null,"code":"import axios from 'axios';\nexport default {\n  beforeCreate() {\n    document.querySelector('body').setAttribute('style', 'background:#9DAB86');\n  },\n\n  beforeUnmount() {\n    document.querySelector('body').setAttribute('style', '');\n  },\n\n  data() {\n    return {\n      currentManga: null\n    };\n  },\n\n  mounted() {\n    axios.get('http://localhost:8082/goodreads/api/animes/' + this.$route.params.id).then(response => {\n      this.currentManga = response.data.data;\n      console.log(response.data);\n    });\n  },\n\n  computed: {\n    manga() {\n      console.log(\"manga computed\");\n      return this.$store.state.manga;\n    }\n\n  },\n  methods: {}\n};","map":{"version":3,"mappings":"AACA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,YAAY,GAAG;IACbC,QAAQ,CAACC,aAAT,CAAuB,MAAvB,EAA+BC,YAA/B,CAA4C,OAA5C,EAAqD,oBAArD;EACD,CAHY;;EAIbC,aAAa,GAAG;IACdH,QAAQ,CAACC,aAAT,CAAuB,MAAvB,EAA+BC,YAA/B,CAA4C,OAA5C,EAAqD,EAArD;EACD,CANY;;EAObE,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE;IADT,CAAP;EAGD,CAXY;;EAYbC,OAAO,GAAG;IACRR,KAAI,CACDS,GADH,CACO,gDAAgD,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAD1E,EAEGC,IAFH,CAESC,QAAD,IAAc;MAClB,KAAKP,YAAL,GAAoBO,QAAQ,CAACR,IAAT,CAAcA,IAAlC;MACAS,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACR,IAArB;IACD,CALH;EAMD,CAnBY;;EAoBbW,QAAQ,EAAE;IACRC,KAAK,GAAG;MACNH,OAAO,CAACC,GAAR,CAAY,gBAAZ;MACA,OAAO,KAAKG,MAAL,CAAYC,KAAZ,CAAkBF,KAAzB;IACF;;EAJQ,CApBG;EA0BbG,OAAO,EAAE;AA1BI,CAAf","names":["axios","beforeCreate","document","querySelector","setAttribute","beforeUnmount","data","currentManga","mounted","get","$route","params","id","then","response","console","log","computed","manga","$store","state","methods"],"sourceRoot":"","sources":["/home/guillaume/Documents/perso/hitema/projet/goodreads-hitema/goodread/src/components/MangaView.vue"],"sourcesContent":["<script>\nimport axios from 'axios'\n\nexport default {\n  beforeCreate() {\n    document.querySelector('body').setAttribute('style', 'background:#9DAB86')\n  },\n  beforeUnmount() {\n    document.querySelector('body').setAttribute('style', '')\n  },\n  data() {\n    return {\n      currentManga: null,\n    }\n  },\n  mounted() {\n    axios\n      .get('http://localhost:8082/goodreads/api/animes/' + this.$route.params.id)\n      .then((response) => {\n        this.currentManga = response.data.data;\n        console.log(response.data)\n      })\n  },\n  computed: {\n    manga() {\n      console.log(\"manga computed\");\n      return this.$store.state.manga;\n    }\n  },\n  methods: {\n  },\n}\n</script>\n\n<template>\n  <div>\n    <h1 v-if=\"currentManga\">{{ currentManga.attributes.canonicalTitle }}</h1>\n    <div class=\"card\">\n      <img v-if=\"currentManga\" :src=\"currentManga.attributes.posterImage.small\" alt=\"Photo d'illustration du manga.\">\n      <h4>\n        Date\n      </h4>\n      <p v-if=\"currentManga\">\n        {{ currentManga.attributes.createdAt }}\n      </p>\n      <h4>\n        Synopsis\n      </h4>\n      <p v-if=\"currentManga\">\n        {{ currentManga.attributes.synopsis }}\n      </p>\n      <h4>\n        Rating\n      </h4>\n      <p v-if=\"currentManga\">\n        {{ currentManga.attributes.averageRating }}\n      </p>\n    </div>\n  </div>\n</template>\n\n<style scoped>\n.card {\n  box-shadow: 10px 5px 5px #c7c7c741;\n  width: auto;\n  margin-bottom: 2rem;\n  border: 1px rgba(0, 0, 0, 0.281) solid;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}