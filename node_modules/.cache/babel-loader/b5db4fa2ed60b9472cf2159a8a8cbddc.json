"/home/guillaume/Documents/perso/hitema/projet/goodreads-hitema/goodread/src/components/AnimeView.vue"],"sourcesContent":["<script>\nimport axios from 'axios'\n\nexport default {\n  beforeCreate() {\n    document.querySelector('body').setAttribute('style', 'background:#9DAB86')\n  },\n  beforeUnmount() {\n    document.querySelector('body').setAttribute('style', '')\n  },\n  /*data() {\n    return {\n      list: [this.$store.dispatch(\"loadAnimes\")],\n      post: {\n        id:\"\",\n      }\n    };\n  },*/\n  data() {\n    return {\n      currentAnime: null,\n      genre: null,\n    }\n  },\n  mounted() {\n    /* \t\tthis.id = this.$route.params.id;\n        this.$store.dispatch('loadAnime', { id: this.$route.params.id })\n        console.log(this.$store.dispatch('loadAnime', { id: this.$route.params.id })) */\n    axios\n      .get('http://localhost:8082/goodreads/api/animes/' + this.$route.params.id)\n      .then((response) => {\n        this.currentAnime = response.data;\n        console.log(this.currentAnime)\n      })\n  },\n  computed: {\n    anime() {\n      return this.$store.state.anime;\n    }\n  },\n  methods: {\n  },\n}\n</script>\n\n<template>\n\n  <div>\n    <h1 v-if=\"currentAnime\">{{ currentAnime.title }}</h1>\n    <div class=\"card\">\n      <img v-if=\"currentAnime\" :src=\"currentAnime.imgSrc\" alt=\"Photo d'illustration du manga.\">\n      <h4>\n        Date\n      </h4>\n      <p v-if=\"currentAnime\">\n        {{ currentAnime.year }}\n      </p>\n      <h4>\n        Synopsis\n      </h4>\n      <p v-if=\"currentAnime\">\n        {{ currentAnime.description }}\n      </p>\n      <h4>\n        Author\n      </h4>\n      <p v-if=\"currentAnime\">\n        {{ currentAnime.author }}\n      </p>\n      <h4>\n        Genre\n      </h4>\n      <p v-if=\"currentAnime\">\n        {{ currentAnime.genres[0].name }}\n      </p>\n      \n    </div>\n  </div>\n</template>\n\n<style scoped>\n.card {\n  box-shadow: 10px 5px 5px #c7c7c741;\n  width: auto;\n  margin-bottom: 2rem;\n  border: 1px rgba(0, 0, 0, 0.281) solid;\n  background-color: #E6A157;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}